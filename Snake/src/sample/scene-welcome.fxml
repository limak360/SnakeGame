<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane maxHeight="600.0" maxWidth="600.0" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0" GridPane.rowIndex="1">
         <children>
            <ImageView fitHeight="610.0" fitWidth="600.0" layoutY="-10.0">
               <image>
                  <Image url="@snake-welcome-background.jpg" />
               </image>
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
            <Button layoutX="300.0" layoutY="140.0" minHeight="30.0" minWidth="150.0" mnemonicParsing="false" onAction="#btnStart" text="Start Game" textAlignment="CENTER" translateX="-75.0" translateY="-15.0" />
            <Button layoutX="300.0" layoutY="200.0" minHeight="30.0" minWidth="120.0" mnemonicParsing="false" onAction="#btnExit" text="Exit" textAlignment="CENTER" translateX="-60.0" translateY="-15.0" />
         </children>
      </Pane>
   </children>
</GridPane>
